<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>{{ h.defaultPageTitle }}</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
</head>

{{#flashes}}

{{#success}}
<div class="card text-white bg-success mb-3" style="max-width: 18rem;">{{.}}</div>
{{/success}}
{{#error}}
<div class="card text-white bg-danger mb-3" style="max-width: 18rem;">{{.}}</div>
{{/error}}
{{#info}}
<div class="card text-white bg-warning mb-3" style="max-width: 18rem;">{{.}}</div>
{{/info}}
{{/flashes}}

<body>
    <div class="container-fluid">
        <table class="table table-striped table-hover table-sm w-50" id="tblPacientesConsultorio2">
            <thead>
                <tr>
                    <th scope="col">Nome Paciente</th>
                    <th scope="col">Prioridade</th>
                    <th scope="col">Chamar</th>
                </tr>
            </thead>
            <tbody>
                {{#fila}}
                    <tr class="linha">
                        <td> {{ nomePaciente }} </td>
                        <td> {{ prioridade }} </td>
                        <td><button class="btn btn-secondary btn-sm">Chamar</button></td>
                        <input type="hidden" value="{{id}}">
                    </tr>
                {{/fila}}
            </tbody>
        </table>
    </div>


    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script type="text/javascript"
        src="https://unpkg.com/speech-synthesis-recorder@1.2.1/SpeechSynthesisRecorder.js"></script>
    <script src="./node_modules/socket.io/client-dist/socket.io.js"></script>
    <script src="./src/dist/js/consultorio2.js"></script>
</body>

</html>